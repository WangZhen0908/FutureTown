        <activity android:name=".activity.UserEnquireActivity" >
        </activity>
        <activity android:name=".activity.OrderManageActivity" >
        </activity>
        <activity android:name=".activity.RegisterActivity" >
        </activity>
        <activity android:name=".activity.ShangjiaCenter" >
        </activity>
        <activity android:name=".activity.EmployeeActivity" >
        </activity>
        <activity android:name=".activity.YifaGoodsManagerActivity" >
